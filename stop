[32m+[m[32m  final List<Transaction> user_transactions = [[m
[32m+[m[32m    Transaction([m
[32m+[m[32m        id: '4544', date: DateTime.now(), price: 46646, title: "Laptop"),[m
[32m+[m[32m  ];[m
[32m+[m[32m  void AddNewTrans(String txtitle, double txtAmount) {[m
[32m+[m[32m    final NewTrans = Transaction([m
[32m+[m[32m        id: DateTime.now().toString(),[m
