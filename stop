[32m+[m[32m  Widget build(BuildContext context) {[m
[32m+[m[32m    return Column([m
[32m+[m[32m      children: [NewTransaction(AddNewTrans), TransactionList(user_transactions)],[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
