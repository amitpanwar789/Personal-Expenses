[32m+[m[32m        title: txtitle);[m
[32m+[m[32m        setState(() {[m
[32m+[m[32m          user_transactions.add(NewTrans);[m
[32m+[m[32m        });[m
[32m+[m[32m  }[m
[32m+[m
